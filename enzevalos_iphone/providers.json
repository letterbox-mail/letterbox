{
    "web":{
        "servers":{
            "imap":[
                {
                    "port":993,
                    "hostname":"imap.web.de",
                    "ssl":true,
                    "auth":"saslPlain"
                }
                     ],
            "smtp":[
                    {
                    "port":587,
                    "hostname":"smtp.web.de",
                    "starttls":true,
                    "auth":"saslPlain"
                    }
                    ]
        },
        "domain-match":[
