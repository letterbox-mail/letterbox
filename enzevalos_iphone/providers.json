{
    "web":{
        "servers":{
            "imap:"
                {
                    "port":993,
                    "hostname":"imap.web.de",
                    "ssl":true
                }
            ,
            "smtp":
                {
                    "port":587,
                    "hostname":"smtp.web.de",
                    "starttls":true
                }
            ]
        },
        "domain-match":[
            //"web\\.de",
            "web\\.de",
            "web"
        ],
        "mx-match": [
            "web",
            "web.de",
            ""
        ]
    },
    "aol":{
        "servers":{
            "imap:"[
                    {
                    "port":993,
                    "hostname":"imap.aol.com",
                    "ssl":true
                    }
                    ],
            "smtp":[
                    {
                    "port":587,
                    "hostname":"smtp.aol.com",
                    "starttls":true
                    }
                    ]
        },
        "domain-match":[
                        //"web\\.de",
                        "aol\\.com",
                        "aol"
                        ],
        "mx-match": [
                     "aol",
                     "aol.com",
                     ""
                     ]
    }
    
}

